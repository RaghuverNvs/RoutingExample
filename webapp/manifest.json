{
	"_version": "1.7.0",
	"sap.app": {
		"id": "RoutingExample",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"

		]
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "RoutingExample.view.Dummy",
			"type": "XML"
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "RoutingExample.view",
				"controlAggregation": "pages",
				"controlId": "dummy",
				"clearControlAggregation": false
			},
			"routes": [{
				"name": "Screen1",
				"pattern": "",
				"target": [
					"Screen1"
				]
			}, {
				"name": "Screen2",
				"pattern": "S2",
				"target": [
					"Screen2"
				]
			}, {
				"name": "Screen3",
				"pattern": "screen3/{enteredEntry}",
				"target": [
					"Screen3"
				]
			}],
			"targets": {
				"Screen1": {
					"viewId": "Screen1",
					"viewName": "S1"
				},
				"Screen2": {
					"viewId": "Screen2",
					"viewName": "S2"
				},
				"Screen3": {
					"viewId": "Screen3",
					"viewName": "S3"
				}
			}
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "RoutingExample.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		}
	}
}